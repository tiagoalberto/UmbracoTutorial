@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<TutorialPage>
@{
	Layout = "master.cshtml";

	await Html.RenderPartialAsync("~/Views/Partials/SectionHeader.cshtml");
}

<section class="section">

	<div class="container">

		<div class="col-md-3">
			<nav class="nav-bar nav-bar--list">
				@{
					await Html.RenderPartialAsync("~/Views/Partials/Navigation/SubNavigation.cshtml");	
				}
			</nav>
		</div>

		<div class="col-md-9">
			<article>
				@await Html.GetBlockListHtmlAsync(Umbraco.AssignedContentItem, "contentBody")
			</article>
		</div>
	</div>

</section>